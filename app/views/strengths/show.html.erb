

  <div class="row">
    <div class="col-xs-2"></div>
    <div class="col-xs-8">
      <div class="sign_up">
        <mark>新規会員登録</mark>
      </div>

      

      <table border="0">
        <tr style="height:70px">
            <th class="form">名前</th>
            <td>
              <%= current_user.last_name %>
              <%= current_user.first_name %>
            </td>
        </tr>

        <tr style="height:70px">
            <th>学年</th>
            <td><%= current_user.grade %></td>
        </tr>

        <tr style="height:70px">
            <th>スキル</th>
            <td><% @strength.skill %></td>
        </tr>

        <tr style="height:70px">
            <th>希望</th>
            <td><% @strength.expection %></td>
        </tr>

        <tr style="height:70px">
            <th>アピールポイント</th>
            <td><% @strength.apeal_point %></td>
        </tr>

        <tr style="height:70px">
            <th>ポートフォリオ</th>
            <td><% @strength.portfolio %></td>
        </tr>

      </table>
      

      
    </div>
  </div>