

  <div class="row">
    <div class="col-xs-2"></div>
    <div class="col-xs-8">
      <div class="sign_up">
        <mark>新規会員登録</mark>
      </div>

      
     <table border="0">
        <tr style="height:70px">
            <th class="form">目標</th>
            <td><%= @setting_goal.target %></td>
        </tr>

        <tr style="height:70px">
            <th>目標達成のための懸念点</th>
            <td><%= @setting_goal.concern %></td>
        </tr>

        <tr style="height:70px">
            <th>コミットメント</th>
            <td><%= @setting_goal.commitment %></td>
        </tr>

      </table>
      
      <%=link_to "編集", edit_user_setting_goal_path(user_id: params[:user_id],id: params[:id]),class: "btn btn-primary", style:"margin-right:30px" %>
      
    </div>
  </div>