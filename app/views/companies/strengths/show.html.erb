

  <div class="row">
    <div class="col-xs-2"></div>
    <div class="col-xs-8">
      <div class="sign_up">
        <mark>新規会員登録</mark>
      </div>

      
     <table border="0">
        <tr style="height:70px">
            <th class="form">名前</th>
            <td><%= @user.user_name %></td>
        </tr>

        <tr style="height:70px">
            <th>学年</th>
            <td><%= @user.grade %></td>
        </tr>

        <tr style="height:70px">
            <th>スキル</th>
            <td><%= @strength.skill %></td>
        </tr>

        <tr style="height:70px">
            <th>希望</th>
            <td><%= @strength.expection %></td>
        </tr>

        <tr style="height:70px">
            <th>アピールポイント</th>
            <td><%= @strength.appeal_point %></td>
        </tr>

        <tr style="height:70px">
            <th>ポートフォリオ</th>
            <td><%= @strength.portfolio %></td>
        </tr>

     </table>
      
      <%=link_to "日々の振り返り", company_company_reviews_path(company_id: current_company.id,id: params[:id]),class: "btn btn-primary", style:"margin-right:30px" %>
      <%=link_to "問題解決", company_company_problem_solutions_path(company_id: current_company.id,id: params[:id]),class: "btn btn-primary", style:"margin-right:30px" %>
      
    </div>
  </div>